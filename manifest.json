{
    "manifest_version": 3,
    "name": "Distraction Blocker",
    "description": "A chrome extension to blur youtube video thumbnails and title",
    "version": "1.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Distraction blocker",
        "default_icon": "block.png"

    }
}